    "def arm_num(n):\n",
    "    num_size=len(str(n))\n",
    "    temp_num=0\n",
    "    arm_num=n\n",
    "\n",
    "    while n>0:\n",
    "        last=n%10\n",
    "        temp_num+=last**num_size\n",
    "        n=n//10\n",
    "    return temp_num==arm_num\n",
    "   \n",
    "print(arm_num(153))"
   ]
  }
